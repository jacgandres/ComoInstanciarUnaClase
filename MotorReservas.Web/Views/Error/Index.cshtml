@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Motor de Reservas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <link href="~/Scripts/Tools/css/style.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/Content/Imgs/kbackgammon-motor-icono-4231-48.ico">

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <h1 style="text-decoration-color:white">Modulo de Errores</h1>
    </div>
    <div class="container body-content">
        <hr />
        <div class="row">
            <div class="page-header remove-margin-top">
                <div class="page-header remove-margin-top">
                    <h1 class="post-title">@ViewBag.Tipo</h1>
                </div>

                <p> 
                    @Html.Raw(ViewBag.Detalle)
                </p>

            </div>
        </div>
    </div>
</body>
</html>
